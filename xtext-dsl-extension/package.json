{
  "name": "@theia/xtext-dsl-extension",
  "keywords": [
    "theia-extension"
  ],
  "version": "0.1.0",
  "files": [
    "lib",
    "src"
  ],
  "dependencies": {
    "@theia/core": "next",
    "@theia/languages": "next"
  },
  "devDependencies": {
    "rimraf": "latest",
    "copyfiles": "latest",
    "typescript": "latest"
  },
  "scripts": {
    "prepare": "yarn run clean && yarn run build",
    "copy-ls": "rimraf build && copyfiles -f ../xtext-railmap-language-server/hu.bme.mit.temalab.railmap.ide/build/libs/railmap-language-server.jar build",
    "clean": "rimraf lib",
    "build": "tsc && yarn run copy-ls",
    "watch": "tsc -w"
  },
  "theiaExtensions": [
    {
      "frontend": "lib/browser/frontend-extension",
      "backend": "lib/node/backend-extension"
    }
  ]
}
